# Auto detect text files and perform LF normalization
* text=auto

# Source code
*.sol text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.json text eol=lf
*.md text eol=lf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.zip binary
*.gz binary

# Exclude from exports
.gitattributes export-ignore
.gitignore export-ignore
.github export-ignore
